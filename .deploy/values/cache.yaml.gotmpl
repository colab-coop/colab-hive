global:
  storageClass: "ceph-filesystem"
nameOverride: "hive-cache"
fullnameOverride: "hive-cache"
master:
  persistence:
    enabled: true
    storageClass: "ceph-filesystem"
    accessModes:
      - ReadWriteMany
    size: 10Gi
replica:
  persistence:
    enabled: true
    storageClass: "ceph-filesystem"
    accessModes:
      - ReadWriteMany
    size: 10Gi
volumePermissions:
  enabled: true
sysctl:
  enabled: true
